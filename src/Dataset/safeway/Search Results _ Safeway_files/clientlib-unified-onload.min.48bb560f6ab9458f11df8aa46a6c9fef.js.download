        			// create object to pass to common update session method
        			let sessionObj = { 'info': { 'COMMON': {}, 'J4U': {}, 'SHOP': {} } };
        			// set storeId to both J4U and SHOP
        			sessionObj.info.J4U.storeId = OrderStoreId;
