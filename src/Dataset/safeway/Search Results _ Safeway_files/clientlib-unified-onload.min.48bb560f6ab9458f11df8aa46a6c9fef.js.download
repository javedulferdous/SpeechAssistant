    this.hasTimeExpired = function(expiry) {
        var hasExpired = false;
        var currentTime = null;
        var expiryTime = null;

        try {
            if (expiry) {
                 currentTime = (new Date()).getTime();
                 expiryTime = new Date(expiry).getTime();
                 hasExpired = currentTime > expiryTime;
            }
        }
        catch (e) {
            hasExpired = true;
        }

        return hasExpired;
    }

