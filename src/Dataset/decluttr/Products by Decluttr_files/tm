			window.$smcCallCustomScripts = function(){$smcT5.CustomScripts = {};try{/*
 * K.O - 2019/12/19 - Timed overlay once scrolled to an element.
 * B.L - 2020/01/23 - Selling basket count
 */
$smcT5.CustomScripts.scrollToTimeTrigger = function timedScrollToTrigger(selector, timeout){
    // Default params
    selector = selector === void(0) ? 'h2[itemprop="price"] > span' : selector;
    timeout = timeout === void(0) ? 3000 : timeout; // Default 3000ms

    var priceElement = document.querySelector(selector);
    
    if(priceElement){
        var priceElementMiddlePosition;
        var bottomOfTheScreen;
        window.addEventListener('scroll', function scrollToTriggerListener(){
            priceElementMiddlePosition = priceElement.offsetTop + (priceElement.offsetHeight / 2);
            bottomOfTheScreen = window.pageYOffset + window.innerHeight;
            if(bottomOfTheScreen >= priceElementMiddlePosition){
                window.removeEventListener('scroll', scrollToTriggerListener);
                setTimeout($smcO5.funcs.displayOverlay, timeout);
            }
        });
    }
};

$smcT5.CustomScripts.TagCode = function() {
    (function sellBasketCount() {
        var url = window.location.href;
        if (url.indexOf('/start-selling/tech-search') > -1) {
            setInterval(function() {
                var count = 0;
                var countEl = document.querySelector('#lblBasketCount');
                if (countEl) {
                    count = Number(countEl.textContent);
                }
                $smcT5.CookieManager.create('smc_sell_basket_count', count, 0);
            }, 1000);
        }
    })();
    
    (function emptyVoucherCode() {
        var searchVoucher = setInterval(function() {
            var input = document.querySelector('input[placeholder="VOUCHER CODE"]');
            if (input) {
                var hasValue = false;
                var value = input.value;
                if (input.value) {
                    hasValue = true;
                }
                
                $smcT5.CookieManager.create('smc_no_voucher', !hasValue, 0);
                
            } else if (!$smcT5.CookieManager.exists('smc_no_voucher')) {
                $smcT5.CookieManager.create('smc_no_voucher', true, 0);
            }
        }, 400);
    })();
    
    (function mobileBasketValue() {
        var search = setInterval(function() {
            var isMobile = false;
            var val = 0;
    
            if (!$smcT5.CookieManager.exists('smc_phone_over_200')) {
                $smcT5.CookieManager.create('smc_phone_over_200', false, 0);
            }
            if (!$smcT5.CookieManager.exists('smc_phone_over_400')) {
                $smcT5.CookieManager.create('smc_phone_over_400', false, 0);
            }
    
            for (var i = 0; i < dataLayer.length; i++) {
                var item = dataLayer[i];
                if (
                    item.valPrice &&
                    item.valCategory &&
                    item.valCategory.toLowerCase().indexOf('phone') > -1
                ) {
                    isMobile = true;
                    val = Number(item.valPrice);
                }
            }
    
            if (isMobile && val >= 200) {
                var buttons = document.querySelectorAll(
                    '.itemConditionSummaryBox .divItemConditionAcceptButton a, .divItemConditionAcceptButton.mobile a'
                );
    
                if (buttons.length) {
                    clearInterval(search);
                    for (var j = 0; j < buttons.length; j++) {
                        var button = buttons[j];
                        button.addEventListener('click', function() {
                            $smcT5.CookieManager.create(
                                'smc_phone_over_200',
                                true,
                                0
                            );
                        });
                    }
                }
            }
            
            if (isMobile && val >= 400) {
                var buttons = document.querySelectorAll(
                    '.itemConditionSummaryBox .divItemConditionAcceptButton a, .divItemConditionAcceptButton.mobile a'
                );
    
                if (buttons.length) {
                    clearInterval(search);
                    for (var j = 0; j < buttons.length; j++) {
                        var button = buttons[j];
                        button.addEventListener('click', function() {
                            $smcT5.CookieManager.create(
                                'smc_phone_over_400',
                                true,
                                0
                            );
                        });
                    }
                }
            }
        }, 400);
    })();
};

//02/10/2020 - J.M - Checks if the product page
$smcT5.CustomScripts.OOSProduct = function(){
    var OOSDetect = document.querySelector('.add-to-cart-button')
    var DLSkuVal = dataLayer[1].ecommerce.detail.products[0].id;
    if (!OOSDetect || OOSDetect.textContent === "Select options to continue"){
        if (DLSkuVal !== undefined){
            var categoryLink = document.querySelector('.breadcrumb li:nth-last-of-type(2) a, h1[itemprop="name"]');
            if (categoryLink.href !== undefined){
                $smcT5.CookieManager.create('smc_category_link', categoryLink.href);
            }
            else{
                $smcT5.CookieManager.create('smc_category_link', categoryLink.textContent);
            }
            
            (function scanBasket(){
                var isDebug = typeof window.$smcDebugger === 'undefined' ? 0 : 1;
                var item = {};
            	var searchInterval = 400; //Time in milliseconds
            
                var checkObjElements = function(obj){
                    var res = true;
                    for (var i = 0; i < Object.keys(obj).length; i++) {
                        var e = Object.keys(obj)[i];
                        if(!obj[e][0]) res = false;
                    }
                    return res;
                }
            
                var diffObject = function(objNew, objOld){
                    var res = false;
                    for (var i = 0; i < Object.keys(objNew).length; i++) {
                        var e = Object.keys(objNew)[i];
                        var old = objOld[e] ? objOld[e][0] : (void 0);
                        if(objNew[e][0] !== old) res = true;
                    }
                    return res;
                }
            	
            	var updateItems = function(){
                    var obj = {};
            		//INSERT SELECTORS HERE --- Format [Element, Property]
                    obj.img = [ document.querySelector('#product-images #main-image img'), "src" ];
                    obj.categoryName = [ document.querySelector('.breadcrumb li:nth-last-of-type(2), h1[itemprop="name"]'), "textContent" ];
                    return obj;
                };
                var newitem = updateItems();
            
            	var updateCookie = function(item){
                    //Get poperties of elements
                    var processedItem = {};
                    for (var key in item) {
                        if (item.hasOwnProperty(key)) processedItem[key] = item[key][0][item[key][1]];
                    }
                    var itemEncoded = btoa(JSON.stringify(processedItem));
            		$smcT5.CookieManager.create('smc_oos_product', itemEncoded, 0);
                }
            
            	var search = setInterval(function(){
            		if(checkObjElements(newitem) && diffObject(newitem, item)){
                        //Update old items
                        item = updateItems();
                        //Update cookie
                        updateCookie(item);
            			if(isDebug) console.log('[SMC] BASKET CHANGED');
                    }
                    newitem = updateItems();
                }, searchInterval);
            })();
            return true;
        }
    }
}}catch( e ){if( 0 ){console.error("SMC Error: "+e );}}};		});
		 });} catch(e){
	//Actual error
	console.log(e);
}
}(window, document));
