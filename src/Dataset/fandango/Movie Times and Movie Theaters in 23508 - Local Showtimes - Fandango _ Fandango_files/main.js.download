///////////////////////////////////////////////////
/*                   ANIMATE                     */
///////////////////////////////////////////////////

var animate = (function() {
	var _this = {};

	
    preloadImages(rmda.productimg, initAnim);

    

    function initAnim() {

        TweenMax.delayedCall(0.5,startAnim);

    }

    
    function startAnim() {
        TweenMax.set('#content_dc',{alpha:1});
        TweenMax.to('#logo',0.5,{alpha:1,ease:Cubic.easeOut});
        TweenMax.to('#price_bg',0.5,{y:0,ease:Power3.easeOut});

        TweenMax.to('#product_holder',1.0,{alpha:1,ease:Cubic.easeOut,delay:0.2});
        TweenMax.to('#info_holder',1.0,{alpha:1,ease:Cubic.easeOut,delay:0.3});
        TweenMax.to('#headline',0.75,{alpha:1,y:0,ease:Power3.easeOut,delay:0.3});
        TweenMax.to('#dot_holder',0.75,{alpha:1,y:0,ease:Power3.easeOut,delay:0.4});

        TweenMax.to('#cta_btn',0.5,{autoAlpha:1,ease:Cubic.easeOut,delay:0.8,onComplete:function(){
            TweenMax.delayedCall(2.0,animateCarousel);
        }});
        
    }
    
    var cycle = 0;
    
    function animateCarousel() {
        //console.log(rmda.currProd,rmda.exitUrls[rmda.currProd]);
        if(rmda.currProd < rmda.dotimg.length-1 && cycle == 0) {
            rmda.currProd++;
            TweenMax.delayedCall(1.5,changeProd);
        }
        else {
            cycle = 1;
            rmda.currProd = 0;
            TweenMax.delayedCall(1.5,changeProd);
        }
    }

    function changeProd() {
        clicks.url = clicks.addSuffix(rmda.exitUrls[rmda.currProd], 'exit_suffix');
        // for dots
        for(var i=0;i<rmda.dotimg.length;i++) {
            util.ele(rmda.dotimg[i]).style.backgroundColor = rmda.inactiveDot;
        }
        util.ele(rmda.dotimg[rmda.currProd]).style.backgroundColor = rmda.activeDot;

        // for name and price
        for(var i=0;i<rmda.productnameprice.length;i++) {
            TweenMax.to(rmda.productnameprice[i],0.5,{alpha:0,ease:Cubic.easeIn});
        }
        TweenMax.to(rmda.productnameprice[rmda.currProd],0.5,{alpha:1,delay:0.3,ease:Cubic.easeOut});

        // for images
        for(var i=0;i<rmda.productimgdiv.length;i++) {
            TweenMax.to(rmda.productimgdiv[i],0.5,{alpha:0,ease:Cubic.easeIn});
        }
        TweenMax.to(rmda.productimgdiv[rmda.currProd],0.5,{alpha:1,delay:0.3,ease:Cubic.easeOut});

        if(cycle == 1) {

        }
        else{

            TweenMax.delayedCall(0.5,animateCarousel);
        }
        
    }
   
    //animateCarousel();
    
    function preloadImages(imgArray, callbackFunction) {
        var totalImages = imgArray.length,
        loadedImages = 0,
        img = null,
        i;
        for (i = 0; i < totalImages; i++) {
            img = document.createElement("img");
            img.src = imgArray[i];
            img.onload = function () {
                loadedImages++;
                if (loadedImages === totalImages) {
                    callbackFunction();
                    console.log("images loaded!");
                    console.log('');
    				console.log('');
                }
            };
        }   
    }

	return _this;

}());



///////////////////////////////////////////////////
/*                 CLICK EVENTS                  */
///////////////////////////////////////////////////

var clicks = (function() {
	var _this = {};

    _this.addSuffix = function(url, dcmParameter) { 
      var urlSuffix = Enabler.getParameter(dcmParameter); 
      var _url = ''; 

      if (url) { 
        var symbol = (url.indexOf('?') > -1) ? '&': '?'; 
        if(urlSuffix) { 
          while (urlSuffix.charAt(0) == '?' || urlSuffix.charAt(0) == '&') { 
            urlSuffix = urlSuffix.substring(1); 
          } 
          if (urlSuffix.indexOf('?') > -1) { 
            urlSuffix = urlSuffix.replace(/\?/g, '&'); 
          } 
        } 
        _url = url + symbol + urlSuffix; 
      } 
      return _url; 
    } 

    _this.url = _this.addSuffix(rmda.exitUrls[rmda.currProd], 'exit_suffix'); 


    util.ele('#background_exit_dc').addEventListener('click',clickEvent,false);
    util.ele('#cta_hotspot').addEventListener('click',clickEvent,false);
    util.ele('#logo_btn').addEventListener('click',clickEvent,false);
    util.ele('#product_exit').addEventListener('click',clickEvent,false);

    util.ele('#cta_btn').addEventListener('mouseover',overEvent,false);
    util.ele('#cta_btn').addEventListener('mouseout',outEvent,false);


    function overEvent(e) {
        TweenMax.to('#cta_btn',0.3,{alpha:0.7,ease:Cubic.easeInOut});
    }

    function outEvent(e) {
        TweenMax.to('#cta_btn',0.3,{alpha:1.0,ease:Cubic.easeInOut});
    }


    function clickEvent(e) {
        
        TweenMax.killAll(true, true, true, true);
        switch(e.target.id) {

            case 'background_exit_dc':
                Enabler.exitOverride('Background Exit',_this.url);
            break;

            case 'cta_hotspot':
                Enabler.exitOverride('CTA exit',_this.url);
            break;

            case 'logo_btn':
                Enabler.exitOverride('Logo exit',_this.url);
            break;

            case 'product_exit':
                Enabler.exitOverride('Product exit',_this.url);
            break;
            
        }

        console.log(_this.url);
    }

    // dot navs
    function activeNavBtn() {
        for(var i=0;i<rmda.dotbtn.length;i++) {
            util.ele(rmda.dotbtn[i]).addEventListener('click',navEvent,false);
            util.ele(rmda.dotbtn[rmda.currProd]).removeEventListener('click',navEvent,false);
        }
    }
        

    function navEvent(e) {
        TweenMax.killAll(true, true, true, true);
        rmda.currProd = Number(e.target.id.substring(8));
        _this.url = _this.addSuffix(rmda.exitUrls[rmda.currProd], 'exit_suffix'); 
        activeNavBtn();

        // for dots
        for(var i=0;i<rmda.dotimg.length;i++) {
            util.ele(rmda.dotimg[i]).style.backgroundColor = rmda.inactiveDot;
        }
        util.ele(rmda.dotimg[rmda.currProd]).style.backgroundColor = rmda.activeDot;

        // for name and price
        for(var i=0;i<rmda.productnameprice.length;i++) {
            TweenMax.to(rmda.productnameprice[i],0.2,{alpha:0,ease:Cubic.easeIn});
        }
        TweenMax.to(rmda.productnameprice[rmda.currProd],0.2,{alpha:1,delay:0.3,ease:Cubic.easeOut});

        // for images
        for(var i=0;i<rmda.productimgdiv.length;i++) {
            TweenMax.to(rmda.productimgdiv[i],0.2,{alpha:0,ease:Cubic.easeIn});
        }
        TweenMax.to(rmda.productimgdiv[rmda.currProd],0.2,{alpha:1,delay:0.3,ease:Cubic.easeOut});

        Enabler.counter('Dot Navigator Clicked');

    }
	
    activeNavBtn();

    


	return _this;

}());