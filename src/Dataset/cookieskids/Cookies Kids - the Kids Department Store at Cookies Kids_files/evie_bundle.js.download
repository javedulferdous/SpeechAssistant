window.evie_cscript = function(active, inner, customsettings, _bundleData, bundler) {

    console.log("evie_cscript", active, inner, customsettings, _bundleData, bundler);

    if (inner) {

        if (location.href.toLowerCase().indexOf("/checkout.aspx") > 0) {
            top.location.href = location.href;
        }

        console.log("evie_bundle preload");
        //--- disable iosScrollable auto

        bundler.iosScrollable("", true);
        bundler.iosScrollable();
        console.log("evie_iphone.js done");

        bundler.sendMessage("pdploaded");

        function setCss(selector, property, value) {
            if(selector) {
                selector.style[property] = value;
            }
        }

        var counter = 10;

        function setInnerCss() {
            if(document.querySelector("header")) {
                innerFrameCss();
            }
            else{
                if(counter > 0) {
                    setTimeout(setInnerCss, 600);
                    counter --;
                }
            }
        }

        setInnerCss();

        function innerFrameCss() {

            var fixedHeader = document.querySelector("header");
            var headerLogoContainer = document.querySelector("[class*='header__logo-container']");
            var cookiesImg = headerLogoContainer.querySelector("a[href='/Default.aspx']");

            if(fixedHeader) {
                cookiesImg.style.marginRight = "5%";
                fixedHeader.querySelector(".mobile-search-cart-container").style.marginRight = "5%";
            }

            setCss(document.getElementById("mainContent"), "marginRight", "5%");
            if(!document.querySelector(".shipping__top-section")) {
                setCss(document.getElementById("mainContent"), "marginLeft", "5%");
            }
            setCss(document.getElementById("navHeader_ibSignInSubmit"), "width", "52%");
            setCss(document.getElementById("MasterMiddle_ibAddToCart"), "width", "12.5rem");
            setCss(document.getElementById("MyRegistryWidgetApiContainer"), "width", "13rem");
            setCss(document.getElementById("footerSignup"), "height", "0.9rem");
            setCss(document.getElementById("MasterMiddle_CartListView1_hlContinueShopping"), "width", "12rem");
            setCss(document.getElementById("MasterMiddle_CartListView1_lbUpdateShippingInfo"), "width", "12rem");
            setCss(document.getElementById("MasterMiddle_lbSubmit"), "width", "65%");
            setCss(document.getElementById("MasterMiddle_wf_GuestPassword"), "width", "95%");
            setCss(document.querySelector(".checkout__help-text"), "marginLeft", "5%");
            $("#MasterMiddle_Billing1_BillingControl_csvContainer div").first().css("width", "37%");
            $(".medium-6").css("width", "95%");
            $(".medium-4").css("width", "95%");
            $(".small-7").css("width", "52%");
            $(".small-up-2>.column").css("width", "44%");
            $(".product-grid__save").css({"height":"1.8rem", "width":"1.8rem"});
            $(".color-swatches").css("height","auto");
            $(".product-information").css("marginLeft","-10px");
            $(".footer__nav-column").css("width","95%");
            $(".sign-up-container").css("marginLeft","-1px");
            $(".sign-up-container").css("width","94%");
            $(".myBuys").css("width","95%");
            setCss(document.getElementById("divCartPopup"), "width", "86%");

            var searchBox = document.querySelector(".yui-ac");
            if(searchBox) {
                searchBox.style.width = "95%";
                searchBox.style.visibility = "hidden";
                const observer = new MutationObserver((mutations) => { 
                    mutations.forEach((mutation) => {
                        searchBox.style.visibility = (searchBox.className != "yui-ac") ? "visible":"hidden";
                    });
                });

                observer.observe(searchBox, { attributes: true, attributeFilter: ["class"] });
            }

        }

    }

    if ( active) {

        var mainPagecounter= 10;

        function setOuterCss() {
            if(document.querySelector(".mobile-search-cart-container")) {
                outerPageCss();
            }
            else{
                if(mainPagecounter > 0) {
                    setTimeout(setOuterCss, 400);
                    mainPagecounter --;
                }
            }
        }

        setOuterCss();

        function outerPageCss() {
            //Mobile Icons
            var mobileSearchContainer = document.querySelector(".mobile-search-cart-container");

            var headerLogoContainer = document.querySelector("[class*='header__logo-container']");
            var cookiesImg = headerLogoContainer.querySelector("a[href='/Default.aspx']");

            var micDivMobile = document.createElement("div");
            micDivMobile.style.display = "inline";

            var micIcon = document.createElement("img");

            micIcon.style.margin = "5px 4px 11px 9.24px";
            micIcon.style.height = "1.65rem";

            function resizeMobileMic() {
                if(mobileSearchContainer && window.getComputedStyle(mobileSearchContainer).display !== "none") {
                    cookiesImg.style.marginRight = window.innerWidth > 340 ? "1.25rem": "3rem";
                    micIcon.style.height = window.innerWidth >= 642 ? "2.95rem": "1.65rem";
                    micIcon.style.margin = window.innerWidth >= 642 ? "0px 4px 15px 9.24px": "5px 4px 11px 9.24px";
                }
            }

            resizeMobileMic();

            var mobileSearch = document.getElementById("mobileSearch");
            mobileSearch.style.marginRight="0.5rem";

            micIcon.src = "data:image/svg+xml;base64,";
            micDivMobile.style.marginRight = "0.4rem";

            micDivMobile.appendChild(micIcon);

            micDivMobile.onclick = function() {
                bundler.sendMessage("searchclick", "prepopulate_search");
            };

            $(micDivMobile).insertBefore(document.getElementById("mobileSearch")).fadeIn();

            //Desktop Icons

            var isIE = (!!window.MSInputMethodContext && !!document.documentMode);
            var isChrome = /Chrome/.test(navigator.userAgent) && /Google Inc/.test(navigator.vendor);

            var searchContainer = document.createElement("div");
            searchContainer.style.marginTop = "-1.70%";

            var searchInput = document.createElement("input");
            searchInput.style.border = ".0625rem solid #eb2024";
            searchInput.style.borderRight= "none";
            searchInput.style.height = "2rem";
            searchInput.style.padding = "0 .2rem 0 .6rem"; 
            searchInput.style.backgroundRepeat = "no-repeat";
            searchInput.style.backgroundSize = "7rem";
            searchInput.style.backgroundPosition = "0.7rem 0.7rem";
            searchInput.style.verticalAlign = "middle";
            searchInput.style.width = isIE? "12.95rem":"10.1rem";

            searchInput.style.backgroundImage = "url('data:image/png;base64,')";

            searchInput.onkeydown = function(e) {
                if( e.keyCode == 13 ) {
                    e.preventDefault(); 
                    keyDiv.click();
                }
            };

            function setSearchInputMargin() {
                if( window.innerWidth < 1200 ) { 
                    searchInput.style.marginLeft = (54.75 - ((1200-window.innerWidth)/16) + "rem");
                } else {	
                    searchInput.style.marginLeft = isChrome? "55.75rem":"56.3rem";
                } 
            }

            setSearchInputMargin();

            window.onresize = function() {
                setSearchInputMargin();
                resizeMobileMic();
            };

            var defaultSearchQueries = [
                "I'm after school shoes for girls",
                "Show me your blue tops under $10",
                "Where are your stores?"
            ];

            var searchList = document.createElement("DATALIST");
            searchList.setAttribute("id", "searchList");
            searchContainer.appendChild(searchList);

            defaultSearchQueries.forEach(function(item) {
                var option = document.createElement("option");
                option.value = item;
                searchList.appendChild(option);
            });

            searchInput.setAttribute("list", "searchList");

            cookiesImg.parentNode.appendChild(searchContainer);
            searchContainer.appendChild(searchInput);
            searchContainer.appendChild(searchList);

            var keyDiv = document.createElement("div");
            var keyBoardImage = document.createElement("img");
            keyBoardImage.style.height = "1.75rem";
            keyBoardImage.style.paddingTop = "0.1875rem";

            keyDiv.style.display = "inline-block";
            keyDiv.style.backgroundColor = "#C33";
            keyDiv.style.height="2.0125rem";
            keyDiv.style.width= isIE? "3.6rem":"3rem";
            keyDiv.style.marginLeft="0rem";
            keyDiv.style.verticalAlign = "middle";
            keyDiv.style.borderRadius = "0 .5rem .5rem 0";
            keyDiv.style.cursor = "pointer";

            keyBoardImage.src = "data:image/svg+xml;base64,";

            keyDiv.appendChild(keyBoardImage);

            searchContainer.appendChild(keyDiv);

            keyDiv.onclick = function(e) {
                if( searchInput.value.trim()!="" ) {
                    bundler.sendMessage("dosearch", searchInput.value);
                }else{
                    bundler.sendMessage("searchclick", "show_keyboard");
                }
            };

            if( !isIE ) {

                var micDiv = document.createElement("div");
                var micImg = document.createElement("img");
                micImg.style.height = "1.8rem";
                micImg.style.paddingTop = "0.1875rem";

                micDiv.style.display = "inline-block";
                micDiv.style.backgroundColor = "#C33";
                micDiv.style.height="2.0125rem";
                micDiv.style.width="3rem";
                micDiv.style.marginLeft="0.4375rem";
                micDiv.style.verticalAlign = "middle";
                micDiv.style.borderRadius = "0.5rem";
                micDiv.style.cursor = "pointer";

                micImg.src = "data:image/svg+xml;base64,";

                micDiv.appendChild(micImg);

                micDiv.onclick = function() {
                    bundler.sendMessage("searchclick", "prepopulate_search");
                };

                searchContainer.appendChild(micDiv);
            }

            searchInput.onfocus = function() {
                searchInput.style.backgroundPosition = "100rem"; 
            };

            searchInput.oninput = function() {
                if( defaultSearchQueries.indexOf(searchInput.value.trim()) != -1 ) {
                    bundler.sendMessage("dosearch", searchInput.value);
                }
            };

            searchInput.onfocusout = function () {
                searchInput.style.backgroundPosition = searchInput.value!="" ? "100rem":"0.7rem 0.7rem";
            };

            function toggleDisplay(x) {
                searchContainer.style.display = watchWidth.matches ? "none":"block";
            }

            var watchWidth= window.matchMedia("(max-width: 992px)");
            toggleDisplay(watchWidth);
            watchWidth.addListener(toggleDisplay);

            toggleZIndices();
            function toggleZIndices() {
                //DOM elements on native site which show on top of the iframe despite lower z-index
                var elements = [".zopim"];
                var chatter = document.getElementById("chatter-button"), maxInvocations = 6;
                resetZindex();
                var observer = new MutationObserver(function(mutations) {
                    mutations.forEach(resetZindex);
                });
                function resetZindex() {
                    if( chatter.style.height == "100%" && chatter.style.height == "100%") {
                        elements.forEach(function(element) {
                            if( $(element).css("z-index") ) {
                                $(element).css("zIndex", -1 * Math.abs(parseInt($(element).css("z-index"))));
                            }else if( maxInvocations != 0 ) {
                                setTimeout(resetZindex, 1000);
                                maxInvocations--;
                            }
                        });
                    }else{
                        elements.forEach(function(element) {
                            $(element).css("zIndex", Math.abs(parseInt($(element).css("z-index"))));
                        });
                    }
                }
                observer.observe(chatter, { attributes : true, attributeFilter : ["style"] });
            }
        }

    }
