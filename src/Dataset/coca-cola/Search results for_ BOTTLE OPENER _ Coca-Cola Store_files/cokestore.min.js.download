define(['jquery','underscore','jquery/ui','mage/translate',],function($,_){'use strict';$.widget('mage.cokestore',{_create:function(config,element){this.intMenu();},intMenu:function(){$('body:not(.store-navigation *)').on('click touch',function(e){if($(window).width()<1200){$('.store-navigation, .store-navigation .active').removeClass('active');}});$('.store-navigation .parent > a').on('click touch',function(e){if($(window).width()<1200){e.stopPropagation();e.preventDefault();$(this).parent().toggleClass('active').siblings().removeClass('active');}})
$('.store-navigation .logo').on('click touch',function(e){e.stopPropagation();if($(window).width()<1200){$('.store-navigation').toggleClass('active').find('.active').removeClass('active');}})}});return $.mage.cokestore;});