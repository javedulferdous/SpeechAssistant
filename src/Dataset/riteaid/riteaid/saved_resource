    <!-- Data Layer to enable rich data collection and targeting -->
    <script type="text/javascript">
        window.dataLayer = window.dataLayer || [];

        try {
            window.dataLayer.push({
                event: 'Page Loaded',
                page: {
                    pageInfo: {
                        pageName: buildPageName(),
                        pageURL: buildPageUrl(),
                        siteDomain: 'web',
                    },
                },
                category: Object.assign({
                    siteSection: 'shop',
                }, buildPageSubSections()),
            });

            _satellite.track('setAAvars');
        } catch (e) {
            console.warn('Unable to track "Page Loaded" event', e);
        }

        /* this code is provided by Adobe team */
        function buildPageName() {
            var pn = '';
            try{ pn = dataLayer.page.pageInfo.pageName || '' } catch (e) { pn = '' }
            if( pn.length>0 ) return pn.replace( /\|/g, ':' );
            pn = location.pathname.split('/');
            pn = pn.filter( function(item) { return item !== '' } );  // remove empty elements from the "pn" array
            if (document.querySelector('body.catalog-product-view') !== null) { // Handle PDP page name
                pn[0] = [pn[0], 'product detail'].join(':');
            }
            return ['web',pn.join(':')].join(':');
        }

        /* this code is provided by Adobe team */
        function buildPageUrl() {
            var pn = location.pathname, r;
            if( pn.length<2 || pn.indexOf( 'index' )>-1 ) pn = '/home';
            r = [ location.hostname.split( '.riteaid.com' )[0], pn ];
            if( location.search.length>0 ) r.push( location.search );
            return r.join('');
        }

        function buildPageSubSections() {
            var siteSubSection = {};
            var pn = location.pathname.replace(/\/shop\//, '');

            // Handle PLP sub sections and all other pages sub sections
            pn = pn.split( '/' );
            pn = pn.filter( function(item) { return item !== '' } );  // remove empty elements from the "pn" array
            var i;
            for (i = 0; i < pn.length; i++) {
                var key = 'siteSubSection' + (i + 1);
                siteSubSection[key] = pn[i];
            }

            if (document.querySelector('body.catalog-product-view') !== null) { // Handle PDP sub sections
                siteSubSection = {
                    siteSubSection1: 'product detail',
                    siteSubSection2: pn
                }
            }
            if (document.querySelector('body.cms-page-view') !== null || document.querySelector('body.cms-home') !== null) { // Handle CMS sub sections
                if( pn.length<2 || pn.indexOf( 'index' )>-1 ) pn = 'home';
                siteSubSection = {
                    siteSubSection1: pn
                }
            }
            return siteSubSection;
        }
    </script>
    <!-- /Data Layer -->

