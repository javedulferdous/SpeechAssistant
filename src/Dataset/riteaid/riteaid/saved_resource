        require(["jquery", './aem', '__webpack_export_common-partial'], function ($, aem) {
            var handleSuccess = function (result) {
                if (result.search('text/javascript') !== -1) {
                    var src,
                        rex = /<script[^>]+src="?([^"\s]+)"?\s*\>/g;

                    while (src = rex.exec(result)) {
                        var source = /^(http|https):/g.exec(src[1]) ? src[1] : ("https:\/\/www.riteaid.com\/" + src[1]);
                        aem(source, 'js', function (result) {
                            let postScript = '; if (navigator.userAgent.indexOf("RiteAidMobile") !== -1) { ' +
                                'window.RiteAid.hideForMobileApp(); ' +
                                'window.RiteAid.enableShopBackButton(); '+
                                '}';
                            var f = new Function('var define = undefined; ' + result + postScript);
                            f = f.bind(window);
                            f();
                        });
                    }
                } else {
                    var html = $.parseHTML(result);
                    $.each(html, function(i, el) {
                        if (el.nodeName.toLowerCase() === 'meta') {
                            $(el).appendTo('head');
                        }

                        if (el.nodeName.toLowerCase() === 'link' && typeof el.href !== 'undefined') {
                            var href = "https:\/\/www.riteaid.com\/" + el.href.replace(/^.*\/\/[^\/]+/, '');
                            $('<link id="aem-style" rel="stylesheet" type="text/css">')
                                .on('load', function () {
                                    $('#aem-header-container, #aem-header-rightrail, #aem-header-leftrail').removeClass('xf-loading');
                                })
                                .attr("href", href)
                                .prependTo('head');
                        }
                    });
                }
            };
                            $.ajax(
                    "https:\/\/www.riteaid.com\/content\/experience-fragments\/riteaid-web\/global\/header\/master.all-js.html",
                    {
                        method: 'get',
                        dataType: 'text',
                        cache: true,
                        error: function (result, code, error) {
                            if (error) {
                                console.log(code + ': ' + error);
                            }
                        },
                        success: handleSuccess,
                    }
                );
                            $.ajax(
                    "https:\/\/www.riteaid.com\/content\/experience-fragments\/riteaid-web\/global\/header\/master.all-css.html",
                    {
                        method: 'get',
                        dataType: 'text',
                        cache: true,
                        error: function (result, code, error) {
                            if (error) {
                                console.log(code + ': ' + error);
                            }
                        },
                        success: handleSuccess,
                    }
                );
