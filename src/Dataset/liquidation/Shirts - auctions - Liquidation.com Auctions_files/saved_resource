function checkMobile(){
	return jQuery.browser.mobile;
}
function isSTR(){
	return (window.location.href.indexOf('direct') != -1);	
}
if(!isSTR() && typeof(AdvancedSearchObj) != "undefined"){
	AdvancedSearchObj.initialize(customSelectFunction);		
