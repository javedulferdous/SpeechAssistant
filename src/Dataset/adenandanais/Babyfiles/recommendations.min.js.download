require(['jquery','mage/url'],function($,url){function addProduct(product){if(!window.productsData){window.productsData={};}
if(product.price){product.price=parseFloat(parseFloat(product.price).toFixed(2));}
if(isNaN(product.price)){product.price=0;}
window.productsData[product.sku]={'id':product.id,'sku':product.sku,'name':product.name,'category':product.category,'brand':window.storeBrand,'price':product.price,'variant':product.variant,'dimension2':product.dimension2,'dimension3':product.dimension3,'dimension11':product.dimension11,'dimension12':product.dimension12,'parentSku':product.parentSku,'parentName':product.parentName,'sizeSelectable':product.sizeSelectable};}
window.productsDataApi=function(skus,callback){let requestUrl=url.build('rest/V1/seo/productsApi');$.get(requestUrl,{skus:skus},function(data){if(data){var response=JSON.parse(data);for(var index in response){if(response.hasOwnProperty(index)){addProduct(response[index]);}}}
if(callback){callback();}});}
if(!window.dataLayer){$(document).on('ga:inited',function(){$(document).trigger('productsDataApi:initialized')});}else{$(document).trigger('productsDataApi:initialized');}});