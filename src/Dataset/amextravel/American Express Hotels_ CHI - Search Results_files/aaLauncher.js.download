        if (typeof d === "undefined" || d == null) {
            continue
        }
        if (d.indexOf("ioaContactPhone") > -1) {
            changeTab(j, "none", "phoneId", "normalTab", null)
        }
        if (d.indexOf("ioaContactMail") > -1) {
            changeTab(j, "none", "mailId", "normalTab", null)
        }
        var m = 20;
        if (d.indexOf("ioaContactOnline") > -1) {
            if (crntActPane == "membershipRewards") {
                m = 20
            }
            changeTab(j, "block", "onlineChatId", "tabHighlighted", m)
        }
    }
    if (onloadlpTestPopulation == "AUTOMATION") {
        var c = document.getElementsByClassName("ioaLCHdr")[0];
        if (c != null) {
            c.innerHTML = "American Express Chat"
        }
        var h = document.getElementsByClassName("ioaLCTxt")[0];
        if (h != null) {
            h.innerHTML = "Get an immediate response to your inquiry."
        }
        var f = document.getElementsByClassName("ioaStrtBtn")[0];
        if (f != null) {
            f.innerHTML = "";
            f.innerHTML = '<a class="ioaOlnBtnLt" href="#" onclick="lpMTagConfig.dynButton1.actionHook();"><span class="ioaOlnBtnMid"><span><img src="https://qwww.aexp-static.com/api/axpi/ioa/img/rsz_chat-bubble-small.png"></span><span style="float: right;">Start Chat</span></span><span class="ioaOlnBtnRt"></span></a>'
        }
    } else {
        var k = window.location.href;
        var c = document.getElementsByClassName("ioaLCHdr")[0];
        if (c != null) {
            if (k.indexOf("travel") > -1 || k.indexOf("iseatz.com") > -1) {
                c.innerHTML = "American Express Travel Chat"
            } else {
                if (k.indexOf("gmip") > -1 || k.indexOf("ATWORK") > -1 || k.indexOf("OSPC") > -1 || k.indexOf("cop") > -1) {
                    c.innerHTML = "American Express Corporate Chat"
                } else {
                    c.innerHTML = "Live Chat"
                }
            }
        }
        var h = document.getElementsByClassName("ioaLCTxt")[0];
        if (h != null) {
            if (k.indexOf("travel") > -1 || k.indexOf("iseatz.com") > -1) {
                h.innerHTML = "Mon-Fri 9am-12am EST, Sat 10am-6:30pm EST and Sun 10am-11pm EST"
            } else {
                if (k.indexOf("gmip") > -1 || k.indexOf("ATWORK") > -1 || k.indexOf("OSPC") > -1 || k.indexOf("cop") > -1) {
                    h.innerHTML = "Available Monday - Friday from 9am - 6pm EST."
                } else {
                    h.innerHTML = "Chat now with one of our Customer Care Professionals."
                }
            }
        }
    }
    adjustMainInfo()
}

