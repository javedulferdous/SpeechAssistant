_satellite.__registerScript('https://assets.adobedtm.com/b714f0d8126e/c733dac6d95d/7f3c9ee05d8b/RC9192c293dd34406e8c509e04ab46fbd2-source.min.js', "<style>\n    #ccpaBannerContainer {\n      position: -webkit-sticky;\n      position: -moz-sticky;\n      position: -ms-sticky;\n      position: -o-sticky;\n      position: sticky;\n      top: 0px;\n      transition: all .5s ease-in-out;\n      width: 100%;\n      text-align: center;\n      background: #f1f1f1;\n      padding: 1em 1em 0 1em;\n      font-size: 15px;\n      z-index: 9;\n    }\n    #ccpaBannerContainer p {\n        display: inline-block;\n    }\n    #ccpaBannerContainer:target {\n        height: 60px;\n    }\n    #ccpaClose {\n        background: none;\n        border: none;\n        float: right;\n        margin-right: 20px;\n    }\n    #ccpaAgree {\n        margin-left: 20px;\n        color: white;\n        border: none;\n        display: inline-block;\n        background-color: #036AD8;\n    }\n    #ccpaBannerContainer a {\n        text-decoration: underline;\n        color: black;\n    }\n    @media only screen and (max-width: 650px) {  \n        #ccpaBannerContainer {\n            text-align: left;\n            font-size: 13px;\n        }\n        #ccpaBannerContainer p {\n            width: 75%;\n        }\n        #ccpaClose {\n            margin-right: 0;\n            font-size: 20px;\n            margin-top: 4px;\n        }\n        #ccpaAgree {\n            position: absolute;\n            height: 30px;\n            right: 60px;\n            margin-top: 5px;\n        }\n    }\n\n    @media only screen and (min-width: 601px) and (max-width: 650px){  \n        #ccpaClose {\n            margin-top: -5px;\n        }\n        #ccpaAgree {\n            margin-top: -5px;\n        }\n    }\n    @media only screen and (min-width: 320px) and (max-width: 600px){  \n        #ccpaAgree {\n            right: 50px\n        }\n        #ccpaClose {\n            margin-right: -16px;\n        }\n    }\n</style>\n<script> \nvar ccpaBanner = {\n    appContainer : document.getElementById('app'),\n    bannerContainer : document.createElement('div'),\n    navContainer : document.querySelector('.footlocker-web-app'),\n    myDate : new Date(),\n    domain : _satellite.getVar('getDomain'),\n    timeoutVar : null,\n    stickyListener : null,\n    ccpaEvent: new CustomEvent('ccpaBannerVisibility'),\n    stickyClass : 'stickyCCPA',\n    bannerContent: '<p id=\"ccpaText\">This site uses tracking cookies to personalize content and ads. '+\n                        '<a href=\"/help/privacy-statement.html\" aria-label=\"learn more about cookie consent\">Learn more</a>'+\n                    '</p>'+\n                    '<button id=\"ccpaAgree\">Accept</button>'+\n                    '<button id=\"ccpaClose\" aria-label=\"Close\">'+\n                    '<span class=\"Icon\">'+\n                        '<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" class=\"injected-svg\" data-src=\"/built/61/icons/base/ic_close.svg\" focusable=\"false\" aria-hidden=\"true\">'+\n                        '<path d=\"M24 1.44L22.59.03 12.01 10.59 1.41 0 0 1.4 10.6 12 0 22.6 1.42 24l10.59-10.58 10.58 10.56 1.41-1.4L13.43 12 24 1.44z\">'+\n                        '</path>'+\n                        '</svg>'+\n                    '</span>'+\n                    '</button>',\n    init: function() {\n        var show = true;\n        var cookieExists = _satellite.cookie.get('ccpa_consent');\n        var domain = _satellite.getVar('getDomain');\n        var md5Email = _satellite.getVar('md5value');\n        if (md5Email){\n            dctfl.log('ccpa: api call');\n            var apiUrl = _satellite.getVar('cpApiUrl') + md5Email;\n            dctfl.apiRequest(apiUrl).then(function(data) {\n                dctfl.log('ccpa: api response', data)\n                if (data && data.ccpa && data.ccpa === \"opted out\") {\n                    var myDate = new Date();\n                    myDate.setMonth(myDate.getMonth() + 12);\n                    if (!cookieExists || cookieExists !== \"refused\") {\n                        document.cookie = \"ccpa_consent=refused;expires=\" + myDate + \";domain=\" + domain + \";path=/;secure\";\n                        dctfl.log(\"ccpa: setting cookie=refused\");\n                    }\n                } else {\n                    ccpaBanner.showBanner();\n                }\n            })[\"catch\"](function(e) {\n                dctfl.log(e);\n            });\n        } else {\n            if (cookieExists){\n                show = false;\n            }\n            if (show){\n                ccpaBanner.showBanner();\n            }\n        }\n    },\n    hideBanner : function(){\n        ccpaBanner.bannerContainer.hidden = true;\n        dctfl.ccpaBannerVisible = false;\n        window.dispatchEvent(ccpaBanner.ccpaEvent);\n\n        },\n    showBanner : function(){\n        ccpaBanner.bannerContainer.setAttribute('tabindex', \"0\");\n        ccpaBanner.bannerContainer.setAttribute(\"aria-labelledby\", \"ccpaText\");\n        ccpaBanner.bannerContainer.setAttribute(\"id\", \"ccpaBannerContainer\");\n        ccpaBanner.bannerContainer.innerHTML = ccpaBanner.bannerContent;\n        ccpaBanner.myDate.setMonth(ccpaBanner.myDate.getMonth() + 12);\n        if (ccpaBanner.navContainer){\n            ccpaBanner.navContainer.insertAdjacentElement('afterbegin', ccpaBanner.bannerContainer)\n            if (document.querySelector('#ccpaBannerContainer')){\n                dctfl.ccpaBannerVisible = true;\n                window.dispatchEvent(ccpaBanner.ccpaEvent);\n                if (window.location.pathname.indexOf('privacy-statement') !== -1 && !ccpaBanner.bannerContainer.hidden){\n                    if (ccpaBanner.domain.indexOf('footaction') !== -1 || ccpaBanner.domain.indexOf('kidsfootlocker') !== -1 ){\n                        location.hash = 'useCookies';\n                    } else {\n                        location.hash = 'cookies';\n                    }\n                    window.scrollBy(0, - ccpaBanner.bannerContainer.offsetHeight * 2)\n                }\n                var closeBtn = document.getElementById('ccpaClose'),\n                agreeBtn = document.getElementById('ccpaAgree'),\n                learnMoreLink = document.querySelector('#ccpaBannerContainer p a');\n                if (closeBtn && agreeBtn) {\n                    closeBtn.addEventListener('click', function() {\n                        _satellite.track('ccpa:close')\n                        document.cookie = \"ccpa_consent=disabled;expires=\"+ccpaBanner.myDate+ \";domain=\" + ccpaBanner.domain + \";path=/;secure\";\n                        ccpaBanner.hideBanner();\n                    });\n                    agreeBtn.addEventListener('click', function() {\n                        _satellite.track('ccpa:agree')\n                        document.cookie = \"ccpa_consent=enabled;expires=\"+ccpaBanner.myDate+\";domain=\" + ccpaBanner.domain + \";path=/;secure\";\n                        ccpaBanner.hideBanner();\n                    });\n                }\n                if (learnMoreLink){\n                    learnMoreLink.addEventListener('click', function(){\n                        _satellite.track('ccpa:privacyLink');\n                    });\n                }\n                \n            }\n        }\n    }\n}\ndctfl.dependsWindowLoaded(ccpaBanner.init, 'ccpa');\nwindow.ccpaBanner = ccpaBanner;\n</script>");