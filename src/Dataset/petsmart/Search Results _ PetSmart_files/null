<script>
// get the URL parameters to know if we need to show or hide the elements on the page
    $.extend({
        getUrlVars: function(){
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for(var i = 0; i < hashes.length; i++)
            {
            hash = hashes[i].split('=');
            vars.push(hash[0]);
            vars[hash[0]] = hash[1];
            }
            return vars;
        },
        getUrlVar: function(name){
            return $.getUrlVars()[name];
        }
    });

    $( document ).ready( function (){
        //find out if a panel is selected, and if so, handle the click process
        if ($.getUrlVar("mpid") != null) {
            var mpid = $.getUrlVar("mpid");
            $('._UT_appBanner__applestoreBadge').attr('href','https://petsmart.haujjd.net/c/'+mpid+'/765825/11083');
            $('._UT_appBanner__androidBadge').attr('href','https://petsmart.haujjd.net/c/'+mpid+'/765825/11083');
            $('.appstore-link').attr('href','https://petsmart.haujjd.net/c/'+mpid+'/765825/11083');
            $('.playstore-link').attr('href','https://petsmart.haujjd.net/c/'+mpid+'/765825/11083');
            $('._HP_appStoreBar__buttonCTA').attr('href','https://petsmart.haujjd.net/c/'+mpid+'/765825/11083');
            $('._HP_appStoreBar__urlLink').attr('href','https://petsmart.haujjd.net/c/'+mpid+'/765825/11083');
            $('._GN_Footer__appButton').attr('href','https://petsmart.haujjd.net/c/'+mpid+'/765825/11083');
        }
});
</script>
</div> 
