<script>
$(document).ready(function () {
    try {
        //See if the url contains the string the user supplies
        if (window.location.search.indexOf('openResetPasswordModal=true') > 0) {
            var passwordReset = $("a.password-reset")[0];

            function getEvents(element) {
                var elemEvents = $._data(element, "events");
                var allDocEvnts = $._data(document, "events");
                for (var evntType in allDocEvnts) {
                    if (allDocEvnts.hasOwnProperty(evntType)) {
                        var evts = allDocEvnts[evntType];
                        for (var i = 0; i < evts.length; i++) {
                            if ($(element).is(evts[i].selector)) {
                                if (elemEvents == null) {
                                    elemEvents = {};
                                }
                                if (!elemEvents.hasOwnProperty(evntType)) {
                                    elemEvents[evntType] = [];
                                }
                                elemEvents[evntType].push(evts[i]);
                            }
                        }
                    }
                }
                return elemEvents;
            }
            
            //All of this is to make sure we wait until the other JS has placed the listener on password-reset
            var checkForListenerInterval = setInterval(checkForListener, 300);

            function checkForListener() {
                if (Object.keys(getEvents(passwordReset)).indexOf('mousedown') > -1) {
                    //If listener is present then clear the interval and click it
                    clearInterval(checkForListenerInterval);
                    $("a.password-reset").trigger('mousedown')
                }
            }
        }
    }
    catch (e) { }
});
</script>
