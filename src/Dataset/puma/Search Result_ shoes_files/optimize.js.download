      "vtp_script":"console.log('Filter Test - Var 1');\nvar startFilterInterval = window.setInterval(function () {\n    if ($('.refinement-toggle').length) {\n        \/\/ Close all filters to start\n        $('.refinement-toggle').each(function () {\n            if ($(this).attr('aria-expanded') !== \"true\") {\n                $(this).click();\n            }\n        })\n\n        \/\/ Set all entries in filtersOpen object to false\n        var filtersOpen = {};\n        $('.refinement-toggle').each(function () {\n            filtersOpen[$(this).attr('href')] = false;\n        });\n\n        \/\/ When clicked, set that entry in filtersOpen to inverse\n        $('.refinement-toggle').on('mousedown', function () {\n            \/\/ this \"if\" prevents changing the object while slider is animating\n            if (!$(this).parent().next().hasClass('collapsing')) {\n                \/\/ Set all entries in filtersOpen to false\n                Object.keys(filtersOpen).forEach(v =\u003E filtersOpen[v] = false);\n                \/\/ Set this entry in filtersOpen to true\n                filtersOpen[$(this).attr('href')] = !filtersOpen[$(this).attr('href')];\n            }\n        });\n\n        var filterInterval = setInterval(function () {\n            var needToAddEventListeners = false;\n            \/\/ For each toggle, if it's open and should not be (based on the object), click it to close\n            $('.refinement-toggle').each(function () {\n                if ($(this).attr('aria-expanded') !== \"false\" \u0026\u0026 filtersOpen[$(this).attr('href')] == false) {\n                    $(this).click();\n                    needToAddEventListeners = true;\n                }\n                if (needToAddEventListeners) {\n                    $('.refinement-toggle').on('mousedown', function () {\n                        if (!$(this).parent().next().hasClass('collapsing')) {\n                            \/\/ Set all entries in filtersOpen to false\n                            Object.keys(filtersOpen).forEach(v =\u003E filtersOpen[v] = false);\n                            \/\/ Set this entry in filtersOpen to true\n                            filtersOpen[$(this).attr('href')] = !filtersOpen[$(this).attr('href')];\n                        }\n                    })\n                    needToAddEventListeners = false;\n                }\n            })\n        }, 1000);\n\n        $('.refinement-bar').css('top', $('.p-header-bar-inner').height() + 10 + 'px');\n        clearInterval(startFilterInterval);\n    }\n}, 250);\n",
