      "vtp_script":"function waitForJquery() {\n    if (window.jQuery) {\n        console.log('Last Viewed Helper Active');\n\n        function readCookie(name) {\n            var nameEQ = name + \"=\";\n            var ca = document.cookie.split(';');\n            for (var i = 0; i \u003C ca.length; i++) {\n                var c = ca[i];\n                while (c.charAt(0) == ' ') c = c.substring(1, c.length);\n                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);\n            }\n            return null;\n        }\n\n        function createCookie(name, value, days) {\n            if (days) {\n                var date = new Date();\n                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));\n                var expires = \"; expires=\" + date.toGMTString();\n            }\n            else var expires = \"\";\n            document.cookie = name + \"=\" + value + expires + \"; path=\/\";\n        }\n\n        var url = window.location.href;\n\n        if (url.includes('\/pd\/')) {\n            setTimeout(function () {\n                if ($('.glide__slide--active').length) {\n                  console.log($('.glide__slide--active').length)\n                    var obj = {\n                        name: $('h1.product-name').text().trim(),\n                        price: $('.is-sale-price').length ? $('.strike-through .value').text().trim() : $('span.value').text().trim(),\n                        url: url,\n                        img: $('.glide__slide--active').length \u003E 1 ? $('.glide-slide-image').last().attr('data-src') : $('.glide-slide-image').first().attr('data-src') || '',\n                        isOnSale: $('.is-sale-price').length ? true : false,\n                        salePrice: $('.is-sale-price').length ? $('.is-sale-price .value').text().trim() : false\n                    }\n                    var productArray = JSON.parse(readCookie('lastViewedProducts')) || [];\n\n                    \/\/return if product is already in the cookie\n                    var alreadyInCookie = false;\n                    \/\/matching based on img so we don't have issues with url query parameters\n                    productArray.forEach(function (product) {\n                        if (product.img == obj.img) {\n                            alreadyInCookie = true;\n                        }\n                    });\n\n                    if (alreadyInCookie) {\n                        return;\n                    }\n\n                    if (productArray.length === 3) {\n                        productArray.push(obj);\n                        productArray.shift();\n                        createCookie('lastViewedProducts', JSON.stringify(productArray), 365);\n                    }\n                    else if (productArray.length == 0) {\n                        createCookie('lastViewedProducts', JSON.stringify([obj]), 365)\n                    }\n                    else {\n                        productArray.push(obj);\n                        createCookie('lastViewedProducts', JSON.stringify(productArray), 365)\n                    }\n                }\n            }, 2000);\n        }\n    } else {\n        setTimeout(function () { waitForJquery() }, 50);\n    }\n}\n\nwaitForJquery();",
