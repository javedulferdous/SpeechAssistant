      "vtp_script":"function waitForJquery() {\n    if (window.jQuery) {\n        \/\/ TEST CHANGES\n      var duration = 7000;\nconsole.log('Last Viewed Test Active');\nfunction readCookie(name) {\n    var nameEQ = name + \"=\";\n    var ca = document.cookie.split(';');\n    for (var i = 0; i \u003C ca.length; i++) {\n        var c = ca[i];\n        while (c.charAt(0) == ' ') c = c.substring(1, c.length);\n        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);\n    }\n    return null;\n}\n\nvar products = [JSON.parse(readCookie('lastViewedProducts')).slice(-1).pop()] || '';\n\nvar interval = setInterval(function () {\n    if ($('body').length \u0026\u0026 !$('.last-viewed').length \u0026\u0026 sessionStorage.getItem('lastViewedShown') != 'true') {\n        if (products) {\n            var productHtml = ''\n            products.forEach(function (product) {\n                \/\/avoid showing product on its own page\n                if (!$('.glide__slide--active').first().find('source').first().attr('data-srcset') || product.img != $('.glide__slide--active').first().find('source').first().attr('data-srcset').split(' ')[0]) {\n                    if (product.isOnSale) {\n                        productHtml += '\u003Ca href=\"' + product.url + '\"\u003E \u003Cimg src=\"' + product.img + '\"\u003E \u003Cdiv class=\"product-info\"\u003E \u003Cspan class=\"title\"\u003E' + product.name + '\u003C\/span\u003E \u003Cdiv class=\"prices\"\u003E\u003Cspan class=\"price sale-price\"\u003E' + product.salePrice + '\u003C\/span\u003E\u003Cspan class=\"price full-price\"\u003E' + product.price + '\u003C\/span\u003E\u003C\/div\u003E \u003C\/div\u003E \u003C\/a\u003E'\n                    }\n                    else {\n                        productHtml += '\u003Ca href=\"' + product.url + '\"\u003E \u003Cimg src=\"' + product.img + '\"\u003E \u003Cdiv class=\"product-info\"\u003E \u003Cspan class=\"title\"\u003E' + product.name + '\u003C\/span\u003E \u003Cspan class=\"price\"\u003E' + product.price + '\u003C\/span\u003E \u003C\/div\u003E \u003C\/a\u003E'\n                    }\n                }\n            })\n\n            if (productHtml) {\n                var container = '\u003Cdiv class=\"last-viewed\"\u003E \u003Cdiv class=\"top-row\"\u003E \u003Cspan class=\"headline\"\u003EPick up where you left off\u003C\/span\u003E\u003Ci class=\"x\"\u003E\u003C\/i\u003E \u003C\/div\u003E \u003Cdiv class=\"middle-row\"\u003E\u003C\/div\u003E\u003C\/div\u003E';\n                $('body').append(container);\n                $('.middle-row').append(productHtml);\n            }\n\n            $('i.x').on('click', function (e) {\n                e.stopPropagation();\n                $('.last-viewed').addClass('slide-out');\n                $('.last-viewed').addClass('clicked');\n            })\n        }\n\n        $('.top-row').on('click', function () {\n            $('.last-viewed').removeClass('clicked');\n            $('.last-viewed').removeClass('slide-out')\n        })\n\n        setTimeout(function () {\n            $('.last-viewed').addClass('slide-out');\n        }, duration);\n        sessionStorage.setItem('lastViewedShown', 'true')\n        clearInterval(interval);\n    }\n}, 250);\n\n    } else {\n        setTimeout(function() { waitForJquery() }, 50);\n    }\n}\nwaitForJquery();\n",
