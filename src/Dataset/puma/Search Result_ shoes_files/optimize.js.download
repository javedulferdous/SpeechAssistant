      "vtp_html":"\u003Cstyle\u003E#collapse-refinement-price {\n    min-width: 300px;\n    min-height: 100px;\n    flex-direction: column-reverse;\n    justify-content: space-around;\n    overflow-x: hidden;\n    padding: 15px;\n}\n\n.refinement-bar-filter-by-title{\n    display: none;\n}\n\n.refinement-toggle{\n    display: none;\n}\n\n.refinement \u003E .collapse{\n    display: none;\n    position: absolute;\n    z-index: 999999;\n    background: white;\n    \/* max-height: 500px; *\/\n    overflow: auto;\n    box-shadow: 0 5px 25px 0 #afa59675, 0 1px 3px 0 #5252524d;\n    top: 35px;\n    left: 0;\n    \/* padding: 5px 10px; *\/\n    text-align: center;\n    padding-bottom: 10px;\n}\n\n.refinements{\n    display: flex;\n    justify-content: flex-start;\n    align-items: center;\n    min-width: 100px;\n}\n\n.refinement-item--text a{\n    white-space: nowrap;\n}\n\n.refinement-list{\n    display: flex;\n    flex-direction: column;\n    align-items: flex-start;\n    max-height: 400px;\n    overflow: auto;\n    display: block;\n    padding: 10px 10px 10px 20px;\n    \/* border-bottom: 1px solid #f0f0f0; *\/\n    margin-bottom: 0;\n}\n\n.refinement{\n    cursor: pointer;\n    border: 1px solid #f0f0f0;\n    border-radius: 3px;\n    margin: 0 3px;\n    position: relative;\n}\n\n.refinement-title{\n    margin: 0;\n    text-transform: capitalize;\n    font-size: .9rem;\n    padding: 4px 28px 4px 10px;\n}\n\n.refinement-title span:after{\n    content: \"\";\n    position: absolute;\n    height: 6px;\n    width: 6px;\n    border-bottom: 2px solid #333;\n    border-right: 2px solid #333;\n    border-radius: 0px 0px 0px 0px;\n    top: 45%;\n    transform: rotate(45deg) translateY(-50%);\n    right: 12px;\n    font-size: 16px;\n}\n\nli.refinement-item--text{\n    text-align: left !important;\n    width: 100%;\n    padding: 1px 0 !important;\n    margin: 0;\n}\n\nli.refinement-item--text:hover{\n    background: #f8f8f8;\n}\n\nli.refinement-item--text a {\n    background: unset !important;\n    font-weight: 600;\n    font-size: 1rem;\n}\n\nli.refinement-item--text a:hover{\n    color: unset;\n}\n\n.refinement \u003E .collapse .refinement-values::-webkit-scrollbar{\n    width: 4px;\n}\n\n.refinement \u003E .collapse .refinement-values::-webkit-scrollbar-track{\n    background: white;\n}\n\n.refinement \u003E .collapse .refinement-values::-webkit-scrollbar-thumb{\n    background: #181818;\n}\n\n.dropdown-active{\n    border-color: #181818;\n}\n\n.dropdown-active .refinement-title span:after{\n    content: \"\";\n    transform: rotate(-135deg);\n    right: -18px;\n}\n\n.refinement-item--color a{\n    border-radius: 50%;\n    outline: none !important;\n    width: 24px !important;\n    height: 24px !important;\n}\n\n#collapse-refinement-color ul{\n    width: 300px;\n    display: grid;\n    grid-template-columns: repeat(7, 1fr);\n    margin: auto;\n    padding: 10px 0;\n}\n\nli.refinement-item--color{\n    margin: 4px 8px !important;\n    line-height: 1px !important;\n}\n\n.swatch-circle-white a{\n    border: 2px solid #ccc !important;\n    width: 26px !important;\n    height: 26px !important;\n}\n\n.refinement--size ul{\n    display: grid !important;\n    grid-template-columns: repeat(8, 1fr);\n}\n\n#collapse-refinement-size li a{\n    width: 45px !important;\n    height: 45px !important;\n    border: 1px solid #b9b9b9;\n    border-radius: 2px;\n    margin: 2px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    padding: 0 !important;\n    line-height: 45px;\n}\n\n#collapse-refinement-size li a{\n    background: none !important;\n    font-weight: 500;\n    color: #181818 !important;\n}\n\n#collapse-refinement-size li a:hover{\n    border-color: #181818;\n}\n\n#collapse-refinement-price .price-range{\n    display: flex;\n    justify-content: space-around;\n    color: #8e8e8e;\n    margin-top: 10px;\n}\n\n.price-range-limits {\n    display: none !important;\n}\n\n.slider-handle{\n    background: #181818 !important;\n    width: 15px !important;\n    height: 15px !important;\n    border-radius: 50%;\n    top: 50%;\n    transform: translateY(-50%) !important;\n}\n\n.slider-handle::after{\n    background: white;\n    width: 3px;\n    height: 3px;\n    content: \"\";\n    display: block;\n    top: 50%;\n    position: absolute;\n    left: 50%;\n    border-radius: 50%;\n    transform: translateX(-50%) translateY(-50%);\n}\n\n.slider-selection{\n    background: #181818 !important;\n    height: 3px !important;\n}\n\n.slider .tooltip.top{\n    margin-top: 30px;\n}\n\n.slider .tooltip.top .tooltip-inner{\n    background: #181818 !important;\n}\n\n.refinement-clear{\n    display: block;\n    text-align: center;\n    border-top: 1px solid #f0f0f0;\n    margin: 0;\n    margin-bottom: -10px;\n    cursor: default;\n}\n\n.refinement-clear a{\n    text-transform: uppercase;\n    font-weight: 800;\n    font-size: 12px;\n    border-bottom: 2px solid #ccc;\n    display: inline-block;\n    padding-bottom: 3px;\n    padding-right: 15px;\n    position: relative;\n    margin: 17px 0 !important;\n}\n\n.refinement-clear a{\n    text-decoration: none !important;\n}\n\n.refinement-clear a::after{\n    content: \"×\";\n    font-size: 18px;\n    position: absolute;\n    line-height: 12px;\n    right: 0;\n}\n\n.refinement li.refinement-item--text a{\n    position: relative;\n    padding-left: 30px !important;\n}\n\n.refinement li.refinement-item--text a:before{\n    content: '';\n    width: 20px;\n    height: 20px;\n    background: none;\n    border: 2px solid #ccc;\n    border-radius: 3px;\n    display: inline-block;\n    position: absolute;\n    top: 50%;\n    transform: translateY(-50%);\n    left: 0px;\n}\n\n.refinement li.refinement-item--text.selected a{\n    background: none !important;\n    color: #333333 !important;\n}\n\n.refinement li.refinement-item--text.selected a:after{\n    content: '';\n    position: absolute;\n    top: 49%;\n    left: 7px;\n    display: inline-block;\n    transform: transla