function showSlider(slider) {
	slider.on('click', function (e) {
		var t = $(this);
		if (t.hasClass('is-active')) {
			t.removeClass('is-active').next().removeClass('reveal-slider');
		} else {
			t.addClass('is-active').next().addClass('reveal-slider');
		}
	});
}

